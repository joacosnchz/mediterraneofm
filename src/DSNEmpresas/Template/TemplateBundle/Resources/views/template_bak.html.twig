<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <meta name="viewport" content="width=device-width, minimum-scale=1.0, maximum-scale=1.0" />
        <title>{% block title %}{% endblock %} - Drift Sudamerica</title>
        {% block css %}
        <!-- Template assets -->
        <link rel="icon" href="{{ asset('favicon.png') }}" type="image/x-icon" />
        <link href="{{ asset('bundles/template/css/template.css') }}" rel="stylesheet" type="text/css" />
        <link href="{{ asset('bundles/template/css/content.css') }}" rel="stylesheet" type="text/css" />
        <link href="{{ asset('bundles/template/css/header.css') }}" rel="stylesheet" type="text/css" />
        <link href="{{ asset('bundles/template/css/menu.css') }}" rel="stylesheet" type="text/css" />
        <link href="{{ asset('bundles/template/css/footer.css') }}" rel="stylesheet" type="text/css" />
        <link href="{{ asset('bundles/template/css/chosen/chosen.css') }}" rel="stylesheet" type="text/css" />
        <link href="{{ asset('bundles/template/css/timepicker/timepicker.css') }}" rel="stylesheet" type="text/css" />
        <!-- Template assets ending -->
        <link href="{{ asset('bundles/template/css/global.css') }}" rel="stylesheet" type="text/css" />
        <link href="{{ asset('bundles/template/css/jquery-ui-1.9.2.custom.css') }}" rel="stylesheet" />
        <link href="{{ asset('bundles/template/css/jquery-ui-1.9.2.custom.min.css') }}" rel="stylesheet" />
        {% if app.session.get('Responsables') %}{% else %}{{ app.session.set('Responsables', 10) }}{% endif %}
        {% if app.session.get('Agencias') %}{% else %}{{ app.session.set('Agencias', 10) }}{% endif %}
        {% if app.session.get('Ciudades') %}{% else %}{{ app.session.set('Ciudades', 10) }}{% endif %}
        {% if app.session.get('Asistentes') %}{% else %}{{ app.session.set('Asistentes', 10) }}{% endif %}
        {% endblock %}

        {# {% include 'TemplateBundle::analytics.html.twig' %} #}
        <script src="{{ asset('bundles/template/js/jquery-1.8.3.js') }}"></script>
        <script src="{{ asset('bundles/template/js/jquery-ui-1.9.2.custom.js') }}"></script>
        <script src="{{ asset('bundles/template/js/chosen/chosen.jquery.js') }}"></script>
        <script src="{{ asset('bundles/template/js/timepicker/jquery-ui-timepicker-addon.js') }}"></script>
        <script>
            $(function() {
                $.datepicker.setDefaults({
                    onSelect: function() {
                        $(this).focus();
                        $(this).datepicker().click(function() {
                            $(this).datepicker('show');
                        });
                    }
                });
            });
        </script>
        {% block js %}
       
        {% endblock %}
    </head>
    <body>
        {% block usermenu %}{% include 'TemplateBundle:Template:user-menu.html.twig' %}{% endblock %}
        
        {% block header %}{% include 'TemplateBundle:Template:header.html.twig' %}{% endblock %}
            
        <div class="wrapper-menu">
            {% block menu %}{% include 'TemplateBundle:Template:menu.html.twig' %}{% endblock %}
        </div>
            
            
        <div class="wrapper-content backcolor1">
            <!-- <div class="wrapper-content-right backcolor3">
                {% block contentRight %}{% endblock %}
            </div> -->

            <div class="wrapper-content-left">
                {% block content %}{% endblock %}
            </div>
        </div>
            

        <footer class="wrapper-footer">
            {% block footer %}{% include 'TemplateBundle:Template:footer.html.twig' %}{% endblock %}
        </footer>
            
    </body>
</html>